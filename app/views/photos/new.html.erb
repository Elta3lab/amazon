<% provide(:title, 'Add New Photo') %>
<h1>Add New Photo</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@photo, url: photos_path, html: { multipart: true }) do |f| %>

      <%= f.label :image %>
      <%= f.file_field :image, class: 'form-control' %>


      <%= f.submit "Submit Photo", class: "btn btn-primary" %>

    <% end %>
  </div>
</div>