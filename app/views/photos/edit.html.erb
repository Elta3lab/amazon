<% provide(:title, 'Edit Photo') %>
<h1>Replace Photo</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@photo, url: @photo, html: { multipart: true }) do |f| %>

      <%= f.label :image %>
      <%= f.file_field :image, class: 'form-control' %>


      <%= f.submit "Submit Photo", class: "btn btn-primary" %>

    <% end %>
  </div>
</div>